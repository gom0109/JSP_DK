<%
	request.setCharacterEncoding("UTF-8");
	String name = request.getParameter("name");
	String age = request.getParameter("address");
	String [] pet = request.getParameterValues("pet");
	
	out.print(name + "<br>");
	out.print(age + "<br>");
	for(int i=0;i<pet.length;i++){
		out.print(pet[i] +" ");
		
	}
%>