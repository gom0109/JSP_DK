<%
	String name = request.getParameter("name");
	String age = request.getParameter("age");
	
%>
이름: <%= name %><br>
나이: <%=age%> <br>

Class Person{
	private String name;
	private String age;
	
	public void setName(String name){
		this.name=name;
	}
	public int getAge(){
		return age;
	}
	
}

Person p = new Person();
p.setName("윤아");